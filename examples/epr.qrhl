# Formalization of one of the examples in "Quantum Relational Hoare Logic" (https://arxiv.org/abs/1802.03188)

isabelle EPR.

quantum var q : bit.
quantum var r : bit.
classical var x : bit.

program left := {
  q,r <q EPR;
  on q apply hadamard;
}.

program right := {
  q,r <q EPR;
  on r apply hadamard;
}.
wp right.
 simp.
wp left.
 simp.
wp right.
 simp.
skip.
simp final_goal.

qed.

